# Production Environment Variables for Netlify

# GROQ Configuration (Required)
GROQ_API_KEY=your_groq_api_key_here

# Application Settings
APP_NAME=Multi-Chatbot Platform
APP_VERSION=1.0.0
NODE_ENV=production
DEBUG=false

# LangChain Configuration
GROQ_MODEL=llama3-8b-8192
MAX_TOKENS=1000
TEMPERATURE=0.7
LOG_LEVEL=INFO

# Netlify Specific
NETLIFY=true
NETLIFY_FUNCTIONS_PORT=8888
